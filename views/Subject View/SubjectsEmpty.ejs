    <table class="subjectTable">
      <thead>
      <tr>
        <th>Name</th>
        <th>Short Name</th>
        <th>Price</th>
        <th>Availibility</th>
        <th>Lecturer</th>
        <th></th>
      </tr>
      </thead>
    </table>
    <p style="color: black; text-align: center">No records found!</p>
    <ul style="list-style-type: none; position: relative; top: 15vh; left: 63vh">
      <li>
        <button onclick="window.location.href='../index.ejs'" class="loginButton" type="submit" style="float: left; display: table; margin: 15px">BACK</button>
      </li>
    </ul>
  </div>
</div>

